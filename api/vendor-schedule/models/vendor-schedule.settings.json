{
  "kind": "collectionType",
  "collectionName": "vendor_schedules",
  "info": {
    "name": "Vendor Schedule"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "startDate": {
      "type": "datetime"
    },
    "endDate": {
      "type": "datetime"
    },
    "client": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "vendor_schedules"
    },
    "vendor": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "vendor_schedules"
    },
    "event": {
      "model": "event",
      "via": "vendor_schedule"
    }
  }
}
